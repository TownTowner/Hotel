{
  "profiles": {
    "Local": {
      "commandName": "Project",
      "launchBrowser": true,
      "applicationUrl": "{Scheme}://localhost:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker Compose": {
      "commandName": "DockerCompose",
      "commandVersion": "1.0",
      "composeLaunchAction": "LaunchBrowser",
      "composeLaunchServiceName": "hotel",
      "composeLaunchUrl": "{Scheme}://localhost:{ServicePort}",
      "serviceActions": {
        "hotel": "StartDebugging"
      }
    }
  }
}